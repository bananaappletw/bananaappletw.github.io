{
    "plugins": [
        "@docusaurus"
    ],
    "extends": [
        "plugin:@docusaurus/recommended",
        "plugin:prettier/recommended",
        "plugin:react/recommended"
    ],
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "env": {
        "es6": true,
        "node": true
    },
    "rules": {
        "react/prop-types": "off"
    },
    "settings": {
        "react": {
            "version": "detect"
        }
    }
}